				</bt:ShortStrings>
				<bt:LongStrings>
					<bt:String id="taskPaneButtonDescription" DefaultValue="Updates Experience directly from email inquiries"/>
				</bt:LongStrings>
			</Resources>
		</VersionOverrides>
	</VersionOverrides>
</OfficeApp>