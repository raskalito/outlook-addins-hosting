											<bt:Image size="32" resid="icon32" />
											<bt:Image size="80" resid="icon80" />
										</Icon>
										<Action xsi:type="ShowTaskpane">
											<SourceLocation resid="messageReadTaskPaneUrl" />
										</Action>
									</Control>
								</Group>
							</OfficeTab>
						</ExtensionPoint>
					</DesktopFormFactor>
				</Host>
			</Hosts>

			<Resources>
				<bt:Images>
					<bt:Image id="icon16" DefaultValue="https://lemon-rock-0d0849e10.1.azurestaticapps.net/Images/Icon16.png"/>
					<bt:Image id="icon32" DefaultValue="https://lemon-rock-0d0849e10.1.azurestaticapps.net/Images/Icon32.png"/>
