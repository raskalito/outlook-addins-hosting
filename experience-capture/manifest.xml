          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
          xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0"
          xsi:type="MailApp">

	<!-- IMPORTANT! Id must be unique for your add-in -->
	<Id>0442beaa-afd6-4434-bc56-8dadf65db0aa</Id>

	<Version>1.0.0.0</Version>
	<ProviderName>HSO</ProviderName>
	<DefaultLocale>en-US</DefaultLocale>
	<!-- The display name of your add-in. Used on the store and various places of the Office UI such as the add-ins dialog. -->
	<DisplayName DefaultValue="Experience Capture" />
	<Description DefaultValue="Updates Experience directly from email inquiries"/>

	<IconUrl DefaultValue="https://lemon-rock-0d0849e10.1.azurestaticapps.net/Images/Icon64.png"/>
	<HighResolutionIconUrl DefaultValue="https://lemon-rock-0d0849e10.1.azurestaticapps.net/Images/Icon80.png"/>

	<SupportUrl DefaultValue="https://lemon-rock-0d0849e10.1.azurestaticapps.net" />
	<!-- Domains that will be allowed when navigating -->
	<AppDomains>
		<AppDomain>apps.powerapps.com</AppDomain>
		<AppDomain>powerapps.com</AppDomain>
		<AppDomain>dynamics.com</AppDomain>
		<AppDomain>office.com</AppDomain>
		<AppDomain>microsoft.com</AppDomain>
	</AppDomains>

	<Hosts>
		<Host Name="Mailbox" />
	</Hosts>
	<Requirements>
		<Sets>
			<Set Name="Mailbox" MinVersion="1.1" />
		</Sets>
	</Requirements>
	<FormSettings>
		<Form xsi:type="ItemRead">
			<DesktopSettings>
				<SourceLocation DefaultValue="https://lemon-rock-0d0849e10.1.azurestaticapps.net/MessageRead.html"/>
				<RequestedHeight>250</RequestedHeight>
			</DesktopSettings>
		</Form>
	</FormSettings>

	<Permissions>ReadWriteItem</Permissions>
	<Rule xsi:type="RuleCollection" Mode="Or">
		<Rule xsi:type="ItemIs" ItemType="Message" FormType="Read" />
	</Rule>

	<VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
		<VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
			<Requirements>
				<bt:Sets DefaultMinVersion="1.3">
					<bt:Set Name="Mailbox" />
				</bt:Sets>
			</Requirements>
			<Hosts>
				<Host xsi:type="MailHost">

					<DesktopFormFactor>
						<ExtensionPoint xsi:type="MessageReadCommandSurface">
							<OfficeTab id="TabDefault">
								<Group id="msgReadGroup">
									<Label resid="groupLabel" />
									<!-- Main Canvas App Button -->
									<Control xsi:type="Button" id="msgReadOpenPaneButton">
										<Label resid="taskPaneButtonLabel" />
										<Supertip>
											<Title resid="taskPaneButtonLabel" />
											<Description resid="taskPaneButtonDescription" />
										</Supertip>
										<Icon>
